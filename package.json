    "@nuxt/eslint": "^0.3.12",
    "@nuxtjs/stylelint-module": "^5.2.0",
    "@stylistic/stylelint-plugin": "^2.1.2",
    "@types/adm-zip": "^0.5.5",
    "@types/geojson": "^7946.0.14",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.10",
    "eslint": "^9.2.0",
    "postcss": "^8.4.38",
